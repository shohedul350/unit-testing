{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "no-underscore-dangle":"off",
        "prefer-destructuring":"off",
        "import/prefer-default-export":"off",
        "max-classes-per-file":"off",
        "class-methods-use-this":"off",
        "no-else-return":"off"
    }
}
